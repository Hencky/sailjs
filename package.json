{
  "name": "sailjs",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "pnpm dev --filter @sailjs/playground",
    "lint": "eslint .",
    "preview": "vite preview",
    "clear": "rimraf 'packages/*/{es,node_modules,coverage}' && rimraf node_modules",
    "prebuild": "npx only-allow pnpm",
    "release": "pnpm changeset publish"
  },
  "dependencies": {
    "@sailjs/core": "workspace:*",
    "antd": "^4.24.15",
    "radash": "^12.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.2.3",
    "vitest": "^2.1.1"
  }
}
